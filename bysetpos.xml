<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/BySetPos"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/bysetpos_label"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/bysetpos_exp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
        
    	<Spinner
        	android:id="@+id/bysetpos_pm"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:ems="2" />

    	<Spinner
        	android:id="@+id/bysetpos_pos"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content" />

    </LinearLayout>
    <Button
        android:id="@+id/bysetpos_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/bymin_add" />

    <Button
        android:id="@+id/bysetpos_clear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/bymin_clear" />

    <TextView
        android:id="@+id/bysetpos_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge" />
	
</LinearLayout>

